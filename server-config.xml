<?xml version="1.0" encoding="UTF-8"?>
<!-- Calimero KNXnet/IP server settings -->
<knxServer name="docker-server" friendlyName="Calimero KNX Server (Docker)">
	<discovery listenNetIf="all" outgoingNetIf="all" />
	<serviceContainer activate="true" routing="true" networkMonitoring="true" listenNetIf="any" udpPort="3671">
		<knxAddress type="individual">1.1.0</knxAddress>
<!--		<knxSubnet type="emulate" medium="tp1">TestNetwork</knxSubnet>-->
		<knxSubnet type="usb" medium="tp1"></knxSubnet>
		<disruptionBuffer expirationTimeout="30" udpPort="5555-5559" />
		<routing>224.0.23.12</routing>
		<groupAddressFilter>
		</groupAddressFilter>
		<additionalAddresses>
			<knxAddress type="individual">1.1.10</knxAddress>
			<knxAddress type="individual">1.1.11</knxAddress>
			<knxAddress type="individual">1.1.12</knxAddress>
			<knxAddress type="individual">1.1.13</knxAddress>
			<knxAddress type="individual">1.1.14</knxAddress>
			<knxAddress type="individual">1.1.15</knxAddress>
		</additionalAddresses>
	</serviceContainer>
</knxServer>
